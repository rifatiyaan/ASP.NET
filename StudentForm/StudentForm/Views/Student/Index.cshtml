@Model List<StudentForm.Models.Entity.Student>
    @{
        ViewBag.Title = "Index";
    }

    <h2>Index</h2>

    <a href="/student/create" class="btn btn-primary">Add Student</a>
    <table class="table table-bordered">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Dob</td>
            <td>Gender</td>
            <td>Cgpa</td>
        </tr>
        @foreach(var s in Model)
            {
                <tr>
                    <td>@s.Id</td>
                    <td>@s.Name</td>
                    <td>@s.Dob</td>
                    <td>@s.Gender</td>
                    <td>@s.Cgpa</td>
                </tr>
            }
    </table>